<br><br><br>
<br>
<h3 class="chead">Categories</h3>
<br>
<% for(var j=0;j<categories.length;j++){ %>
	<span><a href="/doctors/<%= categories[j].category  %>"><span class='li1'><%= categories[j].category %></span></a></span>
<% } %>
<h1 class="head">FindaDoc</h1>
<h3 class="head">Search for local doctors</h3>

<h1 class="add">Add Doctor</h2>
<div class="container">

<form  method="post" action="/adddoctors">
<div class="form-group">
<span class="heading">Full Name:</span>
<input class="form-control" type="text" name='name'><br><br>
</div>
<div class="form-group">
<span class="heading">Category:</span>
<select class="form-control" name="category">
<% for(var j=0;j<categories.length;j++){ %>
	<option  value="<%= categories[j].category %>"><%= categories[j].category %></option>
<% } %>
</select><br><br>
</div>
<div class="form-group">
<span class="heading">Accepting :</span>
<select class="form-control" name="accepting">
<option value='yes'>Yes</option>
<option value="no">No</option>
</select><br><br>
</div>
<div class="form-group">
<span class="heading">Practice Name:</span>
<input class="form-control" type="text" name='pname'><br><br>
</div>
<div class="form-group">
<span class="heading">Graduation Year:</span>
<input class="form-control" type="text" name='year'><br><br>
</div>
<div class="form-group">
<span class="heading">City:</span>
<input class="form-control" type="text" name='city'><br><br>
</div>
<div class="form-group">
<span class="heading">State:</span>
<select class="form-control" name="states">
<option value="Rajasthan">Rajasthan</option>
<option value="Madhya Pradesh">Madhya Pradesh</option>
<option value="Jharkhand">Jharkhand</option>
<option value="Maharashtra">Maharashtra</option>
</select><br>
</div>
<div class="form-group">
<span class="heading">Zipcode:</span>
<input class="form-control" type="text" name='code'><br><br>
</div>
<div class="form-group">
<span class="heading">Address:</span>
<input class="form-control" type="text" name='address'><br><br>
</div>
<div class="form-group">
<span class="heading">Phone Number:</span>
<input class="form-control" type="text" name='phnnumber'><br><br>
</div>
<button type="submit" class="btn btn-success">Add Doctor </button>
<br><br><br>
</form>
</div>
</body>
</html>